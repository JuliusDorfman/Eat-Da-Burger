<div class="container">
    <div class="row">
<div class="jumbotron">
	<h1>Burgers!</h2>
	<h2>Eat-Da-Burger</h2>
</div>
	<ul>
  {{#each burgers}}
      {{ this.burger_name }}
  {{/each}}
	</ul>
	</div>
</div>
<div class="container">

<h2>Add a Burger</h2>
	<div class="row">
		<form class="create-form">
		<div class="form-group">
			<label for="Burger">Burger Name: </label>
			<input type ="text" id="burger_name">
		</div>
		<button id="new-burger" type="submit">Add My Burger</button>
		</form>
	</div>
</div>

<script type="text/javascript">
	$("#new-burger").on("click", function(event) {
		event.preventDefault()
	})
</script>